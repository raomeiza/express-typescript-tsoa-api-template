    "@types/validator": "^13.7.10",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-yaml": "^0.5.0",
    "eslint-plugin-yml": "^0.14.0",
    "flow": "^0.2.3",
    "tsoa": "^5.0.0"
