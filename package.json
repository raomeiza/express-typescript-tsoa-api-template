{
  "name": "peddle",
  "version": "1.0.0",
  "description": "a basic express app writen in typescript",
  "node": ">= 19.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node ./dist/app.js ",
    "start:ts": "ts-node ./src/app.ts --transpile-only",
    "lint": "eslint ./src --ext ts --ext .js --fix",
    "build:dev": "tsoa routes && tsoa spec && tsc -p . --watch",
    "build:watch": "tsc -p . --watch",
    "build": "npm install && tsc",
    "routes": "tsoa routes",
    "spec": "tsoa spec",
    "swagger": "tsoa spec"
  },
  "author": "Abdulraheem A omeiza",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-basic-auth": "^1.2.1",
    "googleapis": "^100.0.0",
    "helmet": "^5.0.2",
    "joi": "^17.6.0",
    "json-bigint": "^1.0.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.3.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.3.0",
    "validator": "^13.7.0",
